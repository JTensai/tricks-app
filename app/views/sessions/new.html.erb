<h1>Log In</h1>

<%= form_tag sessions_path do %>
	<div style="margin: 60px 0px 50px 100px">
		<div class="field">
			<%= label_tag :"email" %><br />
			<%= text_field_tag :email, params[:email] %>
		</div>
		<div class="field">
			<%= label_tag :password %><br />
			<%= password_field_tag :password %>
		</div>
		<br />
		<br />
		<div class="actions"><%= submit_tag "Log In" %></div>
	</div>
<% end %>